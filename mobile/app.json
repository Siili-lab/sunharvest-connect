{
  "expo": {
    "name": "SunHarvest",
    "slug": "sunharvest",
    "version": "1.0.0",
    "scheme": "sunharvest",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sunharvest.connect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sunharvest.connect"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SunHarvest to access your camera for produce grading"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow SunHarvest to access your photos for produce grading"
        }
      ],
      "expo-router"
    ]
  }
}
